{
  "name": "nestjs-boilerplate",
  "version": "1.0.0",
  "description": "The ultimate collection of production-ready and flexible NestJS Boilerplate.",
  "author": {
    "name": "Rizwan Zaheer",
    "email": "rizwan.zaheer678@gmail.com"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run pre-push",
      "commitmsg": "npm run commitmsg"
    }
  },
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "npm run build && npx typeorm -d dist/src/config/db/database.js",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:show": "npm run typeorm -- migration:show",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:drop": "npm run typeorm -- migration:drop",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:migrate": "npm run typeorm -- migration:migrate",
    "schema:sync": "npm run typeorm schema:sync",
    "prepare": "husky install",
    "commitmsg": "sh hooks/commit-msg",
    "pre-push": "npm test",
    "seed:prod": "node dist/seeder/seed.js",
    "seed": "ts-node -r tsconfig-paths/register src/seeder/seed.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@aws-sdk/client-lambda": "^3.758.0",
    "@aws-sdk/client-s3": "^3.408.0",
    "@aws-sdk/client-ses": "^3.408.0",
    "@aws-sdk/s3-request-presigner": "^3.408.0",
    "@aws-sdk/types": "^3.734.0",
    "@casl/ability": "^6.5.0",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.2.4",
    "@nestjs/config": "^3.0.1",
    "@nestjs/core": "^10.2.4",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.2.4",
    "@nestjs/platform-socket.io": "^10.2.4",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.2.7",
    "@sendgrid/mail": "^8.1.3",
    "@sentry/node": "^7.36.0",
    "@sentry/tracing": "^7.36.0",
    "@types/multer": "^1.4.11",
    "@ungap/structured-clone": "^1.2.0",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.2.4",
    "cache-manager-redis-yet": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "configcat-node": "^10.1.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "helmet": "^6.2.0",
    "nestjs-i18n": "^10.4.5",
    "node-mailjet": "^6.0.4",
    "nodemailer": "^6.9.13",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.9.0",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "stripe": "^14.20.0",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.12",
    "typescript": "^4.9.5",
    "uuidv4": "^6.2.13",
    "winston": "^3.10.0",
    "winston-cloudwatch": "^6.2.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^10.2.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.21.0",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.18.8",
    "@types/nodemailer": "^6.4.15",
    "@types/passport": "^1.0.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/pg": "^8.6.6",
    "@types/prettier": "^2.7.3",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/ungap__structured-clone": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "lint-staged": "^14.0.1",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.ts,js,css,md": "prettier --write"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
